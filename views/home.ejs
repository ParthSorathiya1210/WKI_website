    <script>


        window.addEventListener('scroll', function () {
            if (window.scrollY > 2200 && window.innerWidth >= 1300) {
                show();
            }
            else if (window.innerWidth < 1300) {
                show();
            }
        });

        function updateTextPosition() {
            var scrollingText = document.querySelector(".scrollingText");
            window.addEventListener("scroll", updateTextPosition);
            if (window.scrollY > 2000 && window.innerWidth >= 1300) {

                var scrollPosition = window.scrollY;
                var newPosition = 12 + (scrollPosition - 2000) / 14;
                scrollingText.style.left = newPosition + "%";
            }

        }

        function show() {
            let slides = document.getElementsByClassName("shapecard");
            for (let idx = 0; idx < slides.length; idx++) {
                setTimeout(function () {
                    slides[idx].style.opacity = "1";
                }, 400 * idx); // Adjust the time gap (in milliseconds) between each element
            }
        }

        updateTextPosition();
    </script>

